//print Hello, again 5 times
for(int i = 1;  i< 6; i++){
  print ("Hello, again! " + '\n');
}

//print Hello, Again 5 times, and add a count
for(int i = 1;  i< 6;i++){
  print ("Hello, again! " + i +'\n');
}

//print Hello, Again - 1,3,5
for(int i = 1;  i< 6; i+=2){
  print ("Hello, again! " + i +'\n');
}

//print Hello,Again - numbers decending
for(int i = 6;  i> 0; i--){
  print ("Hello, again! " + i +'\n');
}